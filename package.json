{
  "name": "cloak-fast-withdraw",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "tsc",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "start": "node dist/index.js",
    "migrate": "knex --knexfile knexfile.js migrate:latest",
    "migrate:make": "knex --knexfile knexfile.js migrate:make",
    "migrate:rollback": "knex --knexfile knexfile.js migrate:rollback",
    "lint": "prettier --check \"src/**/*.ts\"",
    "lint:fix": "prettier --write \"src/**/*.ts\""
  },
  "dependencies": {
    "commander": "^13.1.0",
    "dotenv": "^17.0.0",
    "ethers": "^6.15.0",
    "knex": "^3.1.0",
    "pg": "^8.16.0",
    "sqlite3": "^5.1.7",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/node": "^24.3.0",
    "prettier": "^3.6.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.0"
  }
}
